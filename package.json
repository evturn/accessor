{
  "name": "accessor",
  "version": "1.0.0",
  "description": "Records inside records",
  "scripts": {
    "start": "NODE_ENV=development babel-node server",
    "check": "npm-check -u",
    "clean:client": "rimraf ./build",
    "clean:server": "rimraf ./lib",
    "clean": "NODE_ENV=production npm run clean:client && npm run clean:server",
    "build:client": "NODE_ENV=production webpack --config config/webpack/webpack.prod.babel.js --color -p",
    "build:server": "NODE_ENV=production mkdir lib && babel server -d lib",
    "prebuild": "npm run clean",
    "build": "npm run build:client && npm run build:server",
    "start:prod": "NODE_ENV=production node lib/admin",
    "login": "firebase login",
    "init": "firebase init",
    "deploy": "firebase deploy"
  },
  "dependencies": {
    "body-parser": "^1.15.2",
    "chalk": "^1.1.3",
    "classnames": "^2.2.5",
    "compression": "^1.6.1",
    "draft-js": "^0.9.1",
    "express": "^4.13.4",
    "firebase": "^3.4.1",
    "firebase-tools": "^3.0.8",
    "history": "^4.3.0",
    "immutable": "^3.8.1",
    "isomorphic-fetch": "^2.2.1",
    "node-uuid": "^1.4.7",
    "npm-check": "^5.2.3",
    "react": "^15.3.2",
    "react-dnd": "^2.1.4",
    "react-dnd-html5-backend": "^2.1.2",
    "react-dom": "^15.3.2",
    "react-redux": "^4.4.5",
    "react-router": "^4.0.0-alpha.4",
    "redux": "^3.6.0",
    "redux-observable": "^0.12.0",
    "rxjs": "^5.0.0-beta.9",
    "sanitize.css": "^4.1.0"
  },
  "devDependencies": {
    "babel-cli": "^6.16.0",
    "babel-core": "^6.17.0",
    "babel-loader": "^6.2.5",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-react-transform": "^2.0.2",
    "babel-plugin-transform-function-bind": "^6.8.0",
    "babel-plugin-transform-react-constant-elements": "^6.8.0",
    "babel-plugin-transform-react-inline-elements": "^6.8.0",
    "babel-plugin-transform-react-remove-prop-types": "^0.2.9",
    "babel-polyfill": "^6.16.0",
    "babel-preset-es2015": "^6.16.0",
    "babel-preset-react": "^6.16.0",
    "babel-preset-react-hmre": "^1.1.1",
    "babel-preset-stage-0": "^6.16.0",
    "css-loader": "^0.25.0",
    "exports-loader": "^0.6.3",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.9.0",
    "html-loader": "^0.4.4",
    "html-webpack-plugin": "^2.16.0",
    "imports-loader": "^0.6.5",
    "json-loader": "^0.5.4",
    "null-loader": "^0.1.1",
    "postcss-cssnext": "^2.8.0",
    "postcss-focus": "^1.0.0",
    "postcss-loader": "^0.13.0",
    "postcss-reporter": "^1.4.1",
    "redux-logger": "^2.6.1",
    "rimraf": "^2.5.4",
    "shelljs": "^0.7.4",
    "style-loader": "^0.13.1",
    "url-loader": "^0.5.7",
    "webpack": "^2.1.0-beta.15",
    "webpack-dev-middleware": "^1.8.3",
    "webpack-hot-middleware": "^2.12.2",
    "write-file-webpack-plugin": "^3.3.0"
  },
  "babel": {
    "presets": [
      "es2015",
      "react",
      "stage-0"
    ],
    "plugins": [
      "add-module-exports",
      "transform-function-bind"
    ],
    "env": {
      "production": {
        "only": [
          "app"
        ],
        "plugins": [
          "transform-react-remove-prop-types",
          "transform-react-constant-elements",
          "transform-react-inline-elements"
        ]
      }
    }
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/evturn/accessor.git"
  },
  "author": {
    "name": "Evan Turner",
    "url": "http://evturn.com",
    "github": "http://github.com/evturn",
    "npm": "https://www.npmjs.com/~evturn"
  },
  "license": "ISC"
}

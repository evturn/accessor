{
  "name": "accessor",
  "version": "1.0.0",
  "description": "Records inside records",
  "scripts": {
    "start": "NODE_ENV=development babel-node server",
    "check": "npm-check -u",
    "clean": "rimraf ./build",
    "prebuild": "npm run clean",
    "build": "NODE_ENV=production webpack --config webpack.config.js --color -p",
    "login": "firebase login",
    "init": "firebase init",
    "deploy": "firebase deploy"
  },
  "dependencies": {
    "classnames": "^2.2.5",
    "firebase": "^3.5.2",
    "firebase-tools": "^3.0.8",
    "npm-check": "^5.2.3",
    "react": "^15.3.2",
    "react-dom": "^15.3.2",
    "react-redux": "^4.4.5",
    "react-router": "next",
    "redux": "^3.6.0",
    "sanitize.css": "^4.1.0"
  },
  "devDependencies": {
    "chalk": "^1.1.3",
    "babel-cli": "^6.16.0",
    "babel-core": "^6.17.0",
    "babel-loader": "^6.2.5",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-react-transform": "^2.0.2",
    "babel-plugin-transform-function-bind": "^6.8.0",
    "babel-plugin-transform-react-constant-elements": "^6.8.0",
    "babel-plugin-transform-react-inline-elements": "^6.8.0",
    "babel-plugin-transform-react-remove-prop-types": "^0.2.9",
    "babel-preset-es2015": "^6.16.0",
    "babel-preset-react": "^6.16.0",
    "babel-preset-react-hmre": "^1.1.1",
    "babel-preset-stage-0": "^6.16.0",
    "css-loader": "^0.25.0",
    "exports-loader": "^0.6.3",
    "express": "^4.13.4",
    "extract-text-webpack-plugin": "1.0.1",
    "file-loader": "^0.9.0",
    "html-loader": "^0.4.4",
    "html-webpack-plugin": "^2.16.0",
    "imports-loader": "^0.6.5",
    "postcss-cssnext": "^2.8.0",
    "postcss-focus": "^1.0.0",
    "postcss-loader": "^0.13.0",
    "postcss-reporter": "^1.4.1",
    "redux-logger": "^2.6.1",
    "rimraf": "^2.5.4",
    "style-loader": "^0.13.1",
    "url-loader": "^0.5.7",
    "webpack": "2.1.0-beta.15",
    "webpack-dev-middleware": "^1.8.3",
    "webpack-hot-middleware": "^2.12.2",
    "write-file-webpack-plugin": "^3.3.0"
  },
  "babel": {
    "presets": [
      "es2015",
      "react",
      "stage-0"
    ],
    "plugins": [
      "add-module-exports",
      "transform-function-bind"
    ],
    "env": {
      "production": {
        "only": [
          "app"
        ],
        "plugins": [
          "transform-react-remove-prop-types",
          "transform-react-constant-elements",
          "transform-react-inline-elements"
        ]
      }
    }
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/evturn/accessor.git"
  },
  "author": {
    "name": "Evan Turner",
    "url": "http://evturn.com",
    "github": "http://github.com/evturn",
    "npm": "https://www.npmjs.com/~evturn"
  },
  "license": "ISC"
}

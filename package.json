{
  "name": "accessor",
  "version": "1.0.0",
  "description": "Records inside records",
  "repository": {
    "type": "git",
    "url": "git://github.com/evturn/accessor.git"
  },
  "author": "Evan Turner",
  "scripts": {
    "prebuild": "npm run test && npm run build:clean",
    "build:clean": "rimraf ./build/*",
    "build": "cross-env NODE_ENV=production webpack --config internals/webpack/webpack.prod.babel.js --color -p",
    "analyze": "node ./internals/scripts/analyze.js",
    "start": "npm run start:tunnel",
    "start:tunnel": "cross-env NODE_ENV=development node server",
    "start:prod": "cross-env NODE_ENV=production node server",
    "pagespeed": "node ./internals/scripts/pagespeed.js",
    "presetup": "npm i chalk",
    "setup": "node ./internals/scripts/setup.js",
    "clean": "shjs ./internals/scripts/clean.js",
    "coveralls": "cat ./coverage/lcov/lcov.info | coveralls"
  },
  "pre-commit": "lint:staged",
  "babel": {
    "presets": [
      "es2015-webpack",
      "react",
      "stage-0"
    ],
    "plugins": [
      "transform-decorators-legacy",
      "transform-function-bind"
    ],
    "env": {
      "production": {
        "only": [
          "app"
        ],
        "plugins": [
          "transform-react-remove-prop-types",
          "transform-react-constant-elements",
          "transform-react-inline-elements"
        ]
      }
    }
  },
  "eslintConfig": {
    "parser": "babel-eslint",
    "env": {
      "browser": true,
      "node": true,
      "mocha": true,
      "es6": true
    },
    "plugins": [
      "react",
      "jsx-a11y"
    ],
    "parserOptions": {
      "ecmaVersion": 6,
      "sourceType": "module",
      "ecmaFeatures": {
        "jsx": true
      }
    }
  },
  "jscsConfig": {
    "preset": "airbnb",
    "validateQuoteMarks": null,
    "requireTrailingComma": null,
    "maximumLineLength": null,
    "maxErrors": null,
    "requireCamelCaseOrUpperCaseIdentifiers": null,
    "requireShorthandArrowFunctions": null,
    "excludeFiles": [
      "build/**",
      "coverage/**",
      "docs/**",
      "node_modules/**"
    ]
  },
  "stylelint": {
    "extends": "stylelint-config-standard",
    "rules": {
      "color-hex-case": "upper",
      "string-quotes": "single",
      "font-family-name-quotes": "single-where-recommended",
      "indentation": 2
    }
  },
  "dependencies": {
    "babel-polyfill": "^6.7.4",
    "chalk": "^1.1.3",
    "compression": "^1.6.1",
    "express": "^4.13.4",
    "history": "^2.1.0",
    "immutable": "^3.8.1",
    "node-uuid": "^1.4.7",
    "react": "^15.0.1",
    "react-dnd": "^2.1.4",
    "react-dnd-html5-backend": "^2.1.2",
    "react-dom": "^15.0.1",
    "react-pure-render": "^1.0.2",
    "react-redux": "^4.4.5",
    "react-router": "^2.4.1",
    "react-router-redux": "^4.0.4",
    "react-router-scroll": "^0.2.0",
    "redux": "^3.5.2",
    "redux-immutable": "^3.0.6",
    "redux-observable": "^0.6.0",
    "reselect": "^2.5.1",
    "rxjs": "^5.0.0-beta.9",
    "sanitize.css": "^3.3.0",
    "whatwg-fetch": "^1.0.0"
  },
  "devDependencies": {
    "babel-core": "^6.7.7",
    "babel-eslint": "^6.0.4",
    "babel-loader": "^6.2.4",
    "babel-plugin-react-transform": "^2.0.2",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-function-bind": "^6.8.0",
    "babel-plugin-transform-react-constant-elements": "^6.8.0",
    "babel-plugin-transform-react-inline-elements": "^6.8.0",
    "babel-plugin-transform-react-remove-prop-types": "^0.2.7",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-es2015-webpack": "^6.4.1",
    "babel-preset-react": "^6.5.0",
    "babel-preset-react-hmre": "^1.1.1",
    "babel-preset-stage-0": "^6.5.0",
    "cheerio": "^0.20.0",
    "coveralls": "^2.11.9",
    "cross-env": "^1.0.7",
    "css-loader": "^0.23.1",
    "enzyme": "^2.2.0",
    "eslint": "^2.10.1",
    "eslint-config-airbnb": "^9.0.1",
    "eslint-import-resolver-webpack": "^0.2.4",
    "eslint-plugin-import": "^1.8.0",
    "eslint-plugin-jsx-a11y": "^1.2.0",
    "eslint-plugin-react": "^5.1.1",
    "eventsource-polyfill": "^0.9.6",
    "exports-loader": "^0.6.3",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.8.5",
    "html-loader": "^0.4.3",
    "html-webpack-plugin": "^2.16.0",
    "imports-loader": "^0.6.5",
    "ip": "^1.1.2",
    "isparta": "^4.0.0",
    "isparta-loader": "^2.0.0",
    "jscs": "^2.11.0",
    "json-loader": "^0.5.4",
    "ngrok": "2.1.8",
    "null-loader": "^0.1.1",
    "offline-plugin": "^3.2.0",
    "plop": "^1.5.0",
    "postcss-cssnext": "^2.5.2",
    "postcss-focus": "^1.0.0",
    "postcss-loader": "^0.9.1",
    "postcss-reporter": "^1.3.3",
    "psi": "^2.0.3",
    "redux-logger": "^2.6.1",
    "rimraf": "^2.5.2",
    "shelljs": "^0.7.0",
    "sinon": "^2.0.0-pre",
    "style-loader": "^0.13.1",
    "stylelint": "^6.3.3",
    "stylelint-config-standard": "^7.0.0",
    "url-loader": "^0.5.7",
    "webpack": "^2.1.0-beta.6",
    "webpack-dev-middleware": "^1.6.1",
    "webpack-hot-middleware": "^2.10.0"
  }
}
